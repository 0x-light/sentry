{
	"name": "sentry-api",
	"main": "worker.js",
	"compatibility_date": "2024-12-01",
	"workers_dev": true,
	// Production route
	"routes": [
		{
			"pattern": "api.sentry.is",
			"custom_domain": true
		}
	],
	// KV namespaces for caching
	"kv_namespaces": [
		{
			"binding": "TWEET_CACHE",
			"id": "8334ef0b45724de8b7beb94690d9e087"
		}
	],
	"observability": {
		"enabled": true
	},
	// Environment variables (set secrets via `wrangler secret put`)
	// Required secrets:
	//   SUPABASE_URL          - Your Supabase project URL
	//   SUPABASE_SERVICE_KEY  - Supabase service role key (for server-side auth)
	//   SUPABASE_ANON_KEY     - Supabase anon key (for client reference)
	//   TWITTER_API_KEY       - twitterapi.io API key
	//   ANTHROPIC_API_KEY     - Anthropic API key
	//   STRIPE_SECRET_KEY     - Stripe secret key
	//   STRIPE_WEBHOOK_SECRET - Stripe webhook signing secret
	//
	// Non-secret env vars:
	// Non-secret env vars:
	// Credit pack Stripe price IDs (one-time):
	//   STRIPE_PRICE_STARTER   - Starter pack ($9, 1K credits)
	//   STRIPE_PRICE_STANDARD  - Standard pack ($39, 5K credits)
	//   STRIPE_PRICE_PRO       - Pro pack ($99, 15K credits)
	//   STRIPE_PRICE_MAX       - Max pack ($199, 40K credits)
	// Credit pack Stripe price IDs (recurring monthly):
	//   STRIPE_PRICE_STARTER_RECURRING
	//   STRIPE_PRICE_STANDARD_RECURRING
	//   STRIPE_PRICE_PRO_RECURRING
	//   STRIPE_PRICE_MAX_RECURRING
	"vars": {
		"STRIPE_PRICE_STARTER": "price_1SyeQ4PhxDggg8ggiC9M7f4H",
		"STRIPE_PRICE_STANDARD": "price_1SyePCPhxDggg8gg8cazPZ3m",
		"STRIPE_PRICE_PRO": "price_1SyeRIPhxDggg8ggTXKkSaaI",
		"STRIPE_PRICE_MAX": "price_1SyeRnPhxDggg8ggz0ECIeLV",
		"STRIPE_PRICE_STARTER_RECURRING": "price_1SyeQWPhxDggg8ggMvv6B4db",
		"STRIPE_PRICE_STANDARD_RECURRING": "price_1SyePpPhxDggg8ggeb3uF45C",
		"STRIPE_PRICE_PRO_RECURRING": "price_1SyeRXPhxDggg8ggbdIv2aMn",
		"STRIPE_PRICE_MAX_RECURRING": "price_1SyeS3PhxDggg8gg7Zt3DgCY",
		"CORS_ORIGIN": "https://sentry.is",
		"ENVIRONMENT": "production"
	}
}